<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <!--Main Window--> <!--  renamed check  -->
    <object id="main_window" class="GtkApplicationWindow">
        <property name="title">main</property>

        <child>
            <object id="main_grid" class="GtkGrid">
                <property name="row-spacing">50</property>
                <property name="column-spacing">10</property>
                <child>
                    <object id="main_title_label" class="GtkLabel">
                        <property name="label">MedOffice</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">0</property>
                            <property name="column-span">4</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="register_btn" class="GtkButton">
                        <property name="label">Register</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">1</property>
                            <property name="column-span">2</property>
                            <property name="row-span">4</property>
                        </layout>
                    </object>
                </child>
                <child>
                    <object id="login_btn" class="GtkButton">
                        <property name="label">Login</property>
                        <layout>
                            <property name="column">3</property>
                            <property name="row">1</property>
                            <property name="column-span">2</property>
                            <property name="row-span">4</property>
                        </layout>
                    </object>
                </child>
            </object>
        </child>
    </object>

<!--Register in Window-->
    <object id="registerWindow" class="GtkApplicationWindow">
        <property name="title">register</property>
        <child>
            <object id="grid_register" class="GtkGrid">
                <property name="row-spacing">10</property>
                <property name="column-spacing">10</property>

                <child>
                    <object id="RegisterCinEntry" class="GtkEntry">
                        <property name="placeholder-text">CIN</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">0</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object id="RegisterFirstNameEntry" class="GtkEntry">
                        <property name="placeholder-text">First Name</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">1</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object id="RegisterLastNameEntry" class="GtkEntry">
                        <property name="placeholder-text">Last Name</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">2</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object id="RegisterAgeEntry" class="GtkEntry">
                        <property name="placeholder-text">Age</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">3</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object id="RegisterAddressEntry" class="GtkEntry">
                        <property name="placeholder-text">Address</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">4</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object id="RegisterPhoneEntry" class="GtkEntry">
                        <property name="placeholder-text">Phone</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">5</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object id="RegisterEmailEntry" class="GtkEntry">
                        <property name="placeholder-text">Email</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">6</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object id="RegisterPasswordEntry" class="GtkEntry">
                        <property name="placeholder-text">Password</property>
                        <property name="visibility">False</property> <!-- Hide password input -->
                        <layout>
                            <property name="column">1</property>
                            <property name="row">7</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object id="confirmPasswordEntry" class="GtkEntry">
                        <property name="placeholder-text">Confirm Password</property>
                        <property name="visibility">False</property> <!-- Hide password input -->
                        <layout>
                            <property name="column">1</property>
                            <property name="row">8</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object id="register" class="GtkButton">
                        <property name="label">register</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">9</property>
                            <property name="column-span">3</property>
                        </layout>
                    </object>
                </child>

                <child>
                    <object id="register_cancel" class="GtkButton">
                        <property name="label">cancel</property>
                        <layout>
                            <property name="column">1</property>
                            <property name="row">10</property>
                            <property name="column-span">3</property>
                        </layout>
                    </object>
                </child>

            </object>
        </child>
    </object>

<!--Login Window-->
    <object id="loginWindow" class="GtkApplicationWindow">
        <property name="title">Login</property>
        <child>
            <object id="grid_login" class="GtkGrid">
